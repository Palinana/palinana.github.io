  },
  "homepage": "https://github.com/Palinana/palinana.github.io#readme",
  "devDependencies": {
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "mapbox-gl": "^0.50.0-beta.1"
  }
}
